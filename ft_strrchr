#include "libft.h"
// EN PROGRESO
char	*ft_strrchr(const char *s, int c)
{
	char	*t_s;

	t_s = s;
	while (*t_s)
	{
		if (*t_s == c)
			return (t_s);
		t_s++;
	}
	return (NULL);
}
